Import-Package: \
    org.osgi.service.cm,\
    org.osgi.framework,\
    org.apache.tamaya,\
    org.apache.tamaya.spi,\
    org.apache.tamaya.functions,\
    org.apache.tamaya.spisupport,\
    org.apache.tamaya.osgi,\
    org.apache.felix.service.command,\
    org.apache.karaf.shell.api.console,\
